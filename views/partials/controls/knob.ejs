<div class="knob left-border" id="<%= name %>">
	<div class="wheel" id="<%= name %>-knob" title="Scroll or drag me up and down, double click to reset">
		<div class="marker"></div>
	</div>
	<label class="value" for="<%= name %>-knob">
		<p class="name">Knob</p>
		<p><output for="<%= name %>-knob">0</output> <span class="units">units</span></p>
	</label>
</div>
